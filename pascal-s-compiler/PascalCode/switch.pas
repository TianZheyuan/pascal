program switch(input,output);
    const luckynum=3;
    var result:integer;
    begin
        if luckynum=1 then
            result:=10-luckynum
        else if luckynum=2 then
            result:=9*luckynum
        else if luckynum=3 then
            result:=luckynum+66
        else if luckynum=4 then
            result:=luckynum-10
    end.